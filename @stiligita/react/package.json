{
  "name": "@stiligita/react",
  "version": "0.0.1-4",
  "main": "lib/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "babel": "babel src --out-dir lib --copy-files",
    "babel:watch": "babel src --out-dir lib --copy-files -w -s",
    "build": "webpack",
    "watch": "webpack-dev-server",
    "test": "bundlesize"
  },
  "bundlesize": [
    {
      "path": "./lib/*elements.js",
      "maxSize": "2 kB"
    },
    {
      "path": "./lib/*{attributes,index,utils}.js",
      "maxSize": "1 kB"
    }
  ],
  "dependencies": {
    "@stiligita/constants": "^0.0.1-1",
    "@stiligita/core": "^0.0.1-2",
    "@stiligita/dom": "^0.0.1-2",
    "@stiligita/hash-code": "^0.0.1-1",
    "@stiligita/keyframes": "^0.0.1-4",
    "@stiligita/store": "^0.0.1-4",
    "@stiligita/stylesheets": "^0.0.1-4",
    "@stiligita/templates": "^0.0.1-1",
    "@stiligita/utils": "^0.0.1-1"
  },
  "devDependencies": {
    "abcq": "^0.2.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "bundlesize": "^0.15.3",
    "cross-env": "^5.1.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "stylis": "^3.4.5",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.5"
  }
}
