{
  "destination": "./docs/api",
  "source": "./@stiligita/",
  "includes": ["\\.js$"],
  "excludes": [
    "lib",
    "dist",
    "node_modules",
    "__tests__",
    "examples",
    "webpack.*.*js"],
  "plugins": [{
    "name": "esdoc-ecmascript-proposal-plugin",
    "option": {
      "all": true
    }
  }, {
    "name": "esdoc-standard-plugin"
  }]
}
